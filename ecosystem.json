{
	"apps" : [
		{
			"name": "[[PROJECT_ID]]",
			"script": "src/server.js",
			"exec_mode": "fork",
			"env": {
			},
			"env_production": {
				"NODE_ENV": "production"
			}
		}
	],
	"deploy" : {
		"production" : {
			"user" : "",
			"host" : "",
			"repo" : "",
			"ref"  : "origin/master",
			"path" : "/var/www/[[REPO_NAME]]",
			"post-deploy" : "npm install --production && pm2 startOrRestart ecosystem.json --env production"
		},
		"development" : {
			"user" : "",
			"host" : "",
			"repo" : "",
			"ref"  : "origin/master",
			"path" : "/var/www/[[REPO_NAME]]",
			"post-deploy" : "npm install && pm2 startOrRestart ecosystem.json --env development"
		}
	}
}
